3E00 |    --     7.751E-03 1.661E-04 3.402E-06 6.783E-08 1.329E-09 2.570E-11 4.923E-13
4E00 |    --     1.322E-02 4.374E-04 1.387E-05 4.282E-07 1.299E-08 3.895E-10 1.157E-11
5E00 |    --     1.812E-02 7.864E-04 3.276E-05 1.330E-06 5.310E-08 2.094E-09 8.181E-11
7E00 |    --     2.595E-02 1.575E-03 9.194E-05 5.236E-06 2.932E-07 1.622E-08 8.896E-10
